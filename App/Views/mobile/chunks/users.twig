{% import '_macros.twig' as macros %}
{% for user in users %}
    <div x='user' param='id={{ user.id }}' class='Navigation_UpdateWin Element__HalfCard Middle Item ToolTipeWrap'>
        <div class='ToolTipe'>{{ user.name }}</div>
        <div class='Ico' style='background-image: url({{ macros.getUserPicture(user.picture) }}); border-radius: 50%;'>
            <div class='Ad'></div>
        </div>
        <div class='Info'>
            <div class='Cut Title ShowTitle'>{{ user.name }}</div>
        </div>
    </div>
{% endfor %}